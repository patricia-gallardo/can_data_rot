                    <DESCRIPTION_CONTRACT_INFORMATION>
                        <TITLE_CONTRACT>
                            <P>Procurement of maintenance and further development contract for Arena.</P>
                        </TITLE_CONTRACT>
                        <TYPE_CONTRACT_LOCATION>
                            <TYPE_CONTRACT VALUE="SERVICES"></TYPE_CONTRACT>
                            <SERVICE_CATEGORY>7</SERVICE_CATEGORY>
                        </TYPE_CONTRACT_LOCATION>
